{
  "name": "ref-back",
  "version": "4.1.1",
  "description": "",
  "repository": "https://github.com/CorentinLat/ref-back",
  "homepage": "https://github.com/CorentinLat/ref-back",
  "author": {
    "name": "Corentin LATAPPY",
    "email": "corentin.lat@gmail.com"
  },
  "license": "GPL-3.0-only",
  "main": "src/electron/main.ts",
  "private": true,
  "scripts": {
    "build": "npm-run-all -p angular:build electron:build",
    "electron": "electron",
    "electron:build": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "electron:local": "npm run build:prod && electron .",
    "electron:serve": "wait-on tcp:4200 && cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "ng": "ng",
    "angular:build": "ng build --base-href ./ -c web-production",
    "angular:serve": "ng serve -c web",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -p never",
    "package:mac": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -m -p never",
    "package:win": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -w -p never",
    "publish": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build -l -m -w -p always",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app"
  },
  "dependencies": {
    "@angular/animations": "13.3.1",
    "@angular/cdk": "13.3.1",
    "@angular/common": "13.3.1",
    "@angular/compiler": "13.3.1",
    "@angular/core": "13.3.1",
    "@angular/forms": "13.3.1",
    "@angular/language-service": "13.3.1",
    "@angular/platform-browser": "13.3.1",
    "@angular/platform-browser-dynamic": "13.3.1",
    "@angular/router": "13.3.1",
    "@ng-bootstrap/ng-bootstrap": "12.1.2",
    "@popperjs/core": "2.11.5",
    "@swimlane/ngx-charts": "20.1.0",
    "@videogular/ngx-videogular": "5.0.1",
    "axios": "1.8.2",
    "bootstrap": "5.1.3",
    "bootstrap-icons": "1.11.3",
    "electron-updater": "6.3.9",
    "ng-multiselect-dropdown": "0.3.9",
    "rxjs": "7.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "13.3.1",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.3.1",
    "@angular/compiler-cli": "13.3.1",
    "@angular/localize": "13.3.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@types/d3-scale": "4.0.2",
    "@types/d3-selection": "3.0.3",
    "@types/fluent-ffmpeg": "2.1.27",
    "@types/node": "17.0.23",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.2",
    "electron": "27.3.11",
    "electron-builder": "25.1.8",
    "electron-debug": "3.2.0",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "1.2.3",
    "electronmon": "2.0.3",
    "eslint": "8.12.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "node-polyfill-webpack-plugin": "4.1.0",
    "npm-run-all": "4.1.5",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.11",
    "ts-loader": "^9.5.1",
    "ts-node": "10.9.2",
    "typescript": "4.6.4",
    "wait-on": "6.0.1",
    "webdriver-manager": "12.1.8",
    "webpack": "5.94.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "4.10.0",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devEngines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/electron/**"
    ],
    "logLevel": "quiet"
  },
  "browserslist": [
    "chrome 100"
  ]
}
